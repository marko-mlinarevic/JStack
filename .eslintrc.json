{
  "env": {
    "jest": true
  },
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ],
  "plugins": [
    "flowtype",
    "compat"
  ],
  "rules": {
    //"semi": [2, "never"],
    "no-unexpected-multiline" : 2,
    "no-multi-spaces": 0,
    "indent": ["error", 4],
    "linebreak-style": 0,
    "compat/compat": 2
  }
}